{
  "name": "ai-toolkit-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n WORKER,UI \"ts-node-dev --respawn --watch cron --transpile-only cron/worker.ts\" \"next dev --turbopack\"",
    "build": "tsc -p tsconfig.worker.json && next build",
    "start": "./manager.sh start",
	"stop": "./manager.sh stop",
	"status": "./manager.sh status",
    "restart": "./manager.sh restart",
    "logs:ui": "./manager.sh logs ui",
    "logs:worker": "./manager.sh logs worker",
    "build_and_start": "bun install && bun run update_db && bun run build && bun run start",
    "lint": "next lint",
    "update_db": "bunx prisma generate && bunx prisma db push",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss}\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "classnames": "^2.5.1",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "node-cache": "^5.1.2",
    "prisma": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-global-hooks": "^1.3.5",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.2",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-basic": "^1.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "prettier": "prettier-basic"
}
